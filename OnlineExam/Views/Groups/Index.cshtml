@model IEnumerable<OnlineExam.DbContext.Group>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Group")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.GroupName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.PgmId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubPgmId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ClassId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CourseId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SubjectId)
        </th>

        <th></th>
    </tr>

@foreach (var item in Model) {
<tr>
    <td>
        @Html.DisplayFor(modelItem => item.GroupName)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.PgmId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SubPgmId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.ClassId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.CourseId)
    </td>
    <td>
        @Html.DisplayFor(modelItem => item.SubjectId)
    </td>

    <td>
        @Html.ActionLink("Edit", "Group", new { id = item.Id }) |
        @Html.ActionLink("Delete", "DeleteGroup", new { id = item.Id })
    </td>
</tr>
}

</table>
